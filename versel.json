{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist/smart_point_electronica/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://smartpoint-api.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/smart_point_electronica/browser/index.html"
    }
  ],
  "env": {
    "API_BASE_URL": "https://smartpoint-api.onrender.com"
  }
}